// https://leetcode.com/problems/airplane-seat-assignment-probability/description/

class Solution {
public:
    double nthPersonGetsNthSeat(int n) {
        if(n==1) return (double)1;
        else return(double)1/2;
    }
};

// https://leetcode.com/problems/airplane-seat-assignment-probability/submissions/872188542/
